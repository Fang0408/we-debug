  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dlhandsome/we-debug/issues"
  },
  "homepage": "https://github.com/dlhandsome/we-debug#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-modules-commonjs": "^7.9.0",
    "autoprefixer": "^9.7.5",
    "cross-env": "^7.0.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-less": "^4.0.1",
    "gulp-postcss": "^8.0.0",
